<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		<!-- Dans le fichier Twig où vous souhaitez utiliser les icônes Material Icons -->
		<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"> {% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body class="bg-gray-200">
        {% block body %}{% endblock %}

		{% for type, flashMessages in app.flashes %}
			{% for message in flashMessages %}
				{{ type }}
				-
				{{ message }}
			{% endfor %}
		{% endfor %}
	</body>
</html>
